//PROJECT DOG YEARS
var myAge = 27;
//this is my age

var earlyYears = 2;
//this is 2

// var earlyYears = console.log(earlyYears*10.5); 
var earlyYears = (earlyYears*10.5);

var laterYears = (myAge - 2); 
//this is my age - 2

console.log(laterYears*2);
console.log(earlyYears*10.5);

var myAgeInDogYears = ((laterYears*2)) + ((earlyYears*10.5));
console.log(myAgeInDogYears);
//my age in dog years

var myName = 'TYLER';
var myName = (myName.toLowerCase());
// I set the variable as my name with the lowercase method, so that when it's entered into the line of code below it will output as all lowercase in the console

console.log(`My name is ${(myName)}. I am ${myAge} years old in human years which is ${myAgeInDogYears} years old in dog years.`);
// displays my name and age in dog years using string interpolation
